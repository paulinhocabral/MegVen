<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="consauditoria" table="consauditoria" schema="public" optimistic-lock="version">
        <id name="codigo" type="int">
            <column name="codigo" />
            <generator class="increment" />
        </id>
        <property name="datahora" type="timestamp">
            <column name="datahora" length="45" not-null="true" />
        </property>
        <property name="acao" type="string">
            <column name="acao" length="45" not-null="true" />
        </property>
        <property name="valoranterior" type="string">
            <column name="valoranterior" length="65535" not-null="true" />
        </property>
        <property name="valorposterior" type="string">
            <column name="valorposterior" length="65535" not-null="true" />
        </property>
        <property name="usuario_codigo" type="int">
            <column name="usuario_codigo" not-null="true" />
        </property>        
    </class>  
</hibernate-mapping>
